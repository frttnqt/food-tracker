{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "tabs",
      2
    ],
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "object-curly-spacing": true,
    "ordered-imports": false,
    "no-console": false,
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature"
    ]
  },
  "rulesDirectory": []
}
